{
  "name": "drizzle-cuid2",
  "license": "MIT",
  "version": "0.0.0",
  "description": "A library for generating cuid2 id columns for drizzle",
  "exports": {
    "./mysql": {
      "import": "./dist/mysql-core/index.mjs",
      "require": "./dist/mysql-core/index.js",
      "types": "./dist/mysql-core/index.d.ts"
    },
    "./sqlite": {
      "import": "./dist/sqlite-core/index.mjs",
      "require": "./dist/sqlite-core/index.js",
      "types": "./dist/sqlite-core/index.d.ts"
    },
    "./postgres": {
      "import": "./dist/postgres-core/index.mjs",
      "require": "./dist/postgres-core/index.js",
      "types": "./dist/postgres-core/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "tsc"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.5",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "drizzle-orm": "^0.31.1"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2"
  }
}
